packages:
  effectful-core/*.cabal
  effectful-plugin/*.cabal
  effectful-th/*.cabal
  effectful/*.cabal

-- GHC 9.4
allow-newer: freer-simple:template-haskell

-- GHC 9.6
allow-newer: async:base
           , cleff:base
           , cleff:template-haskell
           , fused-effects:base
           , fused-effects:transformers
           , hashable:base
           , hashable:ghc-prim
           , polysemy:transformers
           , unliftio-core:base
