packages:
  effectful-core/*.cabal
  effectful-plugin/*.cabal
  effectful-th/*.cabal
  effectful/*.cabal

-- Ignore dependencies of other effect libraries.
allow-newer: cleff:*
           , freer-simple:*
           , fused-effects:*
           , polysemy:*

-- GHC 9.8
allow-newer: *:base
           , *:bytestring
           , *:deepseq
           , *:template-haskell
           , *:text
